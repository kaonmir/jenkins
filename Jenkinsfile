pipeline {
  agent any
  stages {
    stage('print') {
      agent any
      steps {
        sh '''sh "echo rtcyvhjbknk"
ls'''
      }
    }

    stage('') {
      parallel {
        stage('hj') {
          agent any
          steps {
            sleep 3
          }
        }

        stage('lknp') {
          agent any
          steps {
            echo 'gds'
          }
        }

      }
    }

  }
}