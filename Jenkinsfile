pipeline {
  agent any
  stages {
    stage('print') {
      agent any
      steps {
        sh '''sh "echo rtcyvhjbknk"
ls'''
      }
    }

    stage('error') {
      parallel {
        stage('hj') {
          agent any
          steps {
            sleep 3
          }
        }

        stage('lknp') {
          agent any
          steps {
            echo 'gds'
          }
        }

      }
    }

    stage('errors') {
      parallel {
        stage('jhvhj') {
          steps {
            echo ' jl'
          }
        }

        stage('net') {
          steps {
            echo 'hgi'
          }
        }

        stage('bae') {
          steps {
            echo 'ent'
          }
        }

      }
    }

    stage('fymsmf') {
      steps {
        echo 'agnadg'
      }
    }

  }
}